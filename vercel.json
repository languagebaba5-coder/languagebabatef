{
  "version": 2,
  "functions": {
    "server.js": {
      "runtime": "nodejs18.x"
    },
    "api/blog.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/blog-test",
      "destination": "/blog-test.html"
    },
    {
      "source": "/blog-test.html",
      "destination": "/blog-test.html"
    },
    {
      "source": "/blog",
      "destination": "/api/blog"
    },
    {
      "source": "/blog.html",
      "destination": "/api/blog"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/admin.html",
      "destination": "/admin.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}